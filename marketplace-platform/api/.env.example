# JARVIS Premium License API Configuration

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here  
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Gumroad Integration (Alternative to Stripe)
GUMROAD_ACCESS_TOKEN=your_gumroad_token_here

# Frontend URL (for checkout redirects)
FRONTEND_URL=https://premium.jarvis.ai

# API Configuration
JWT_SECRET=your_jwt_secret_for_tokens
API_BASE_URL=https://api.jarvis.ai

# Database (for production)
DATABASE_URL=postgresql://username:password@host:5432/jarvis_premium

# Email Service (for license delivery)
SENDGRID_API_KEY=your_sendgrid_key_here
FROM_EMAIL=noreply@jarvis.ai

# Security
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=900000

# Analytics
ANALYTICS_ENABLED=true
REVENUE_TRACKING_ENABLED=true